{
  "name": "sanity-nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "node export/exportSymbols.mjs",
    "import:symbols": "sanity dataset import ./symbols.ndjson --replace --allow-failing-assets",
    "import:types": "sanity dataset import ./type.ndjson --replace --allow-failing-assets",
    "import:affiliations": "sanity dataset import ./affiliation.ndjson --replace --allow-failing-assets",
    "import:symbol": "sanity dataset import ./symbol.ndjson --replace --allow-failing-assets",
    "delete:symbols": "sanity documents query \"*[_type == 'post']._id\" | groq \"*\" -o ndjson | xargs sanity documents delete",
    "postbuild": "node algolia/index.mjs"
  },
  "dependencies": {
    "@sanity/client": "^6.12.4",
    "@sanity/export": "^3.36.4",
    "@sanity/icons": "^2.8",
    "@sanity/image-url": "1",
    "@sanity/types": "^3.25",
    "@sanity/ui": "^2.0",
    "@sanity/vision": "3",
    "algoliasearch": "^4.23.2",
    "dotenv": "^16.4.5",
    "front-matter": "^4.0.2",
    "jsdom": "^24.0.0",
    "markdown-it": "^14.1.0",
    "next": "^14.1",
    "next-sanity": "7.1.4",
    "react": "^18.2",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-player": "^2.15.1",
    "react-schemaorg": "^2.0.0",
    "sanity": "^3.25",
    "sanity-plugin-singleton-tools": "^1.0.1",
    "sanity-plugin-vercel-deploy": "^3.3.4",
    "schema-dts": "^1.1.2",
    "showdown": "^2.1.0",
    "slugify": "^1.6.6",
    "styled-components": "^6.0",
    "title-case": "^4.3.1"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
